[project]
	description = Unofficial cm10.1
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/beast1980
[access "refs/for/refs/*"]
	read = group user/beast1980
	push = group user/beast1980
	label-Code-Review = -2..+2 group user/beast1980
	label-Verified = -1..+1 group user/beast1980
	rebase = group user/beast1980
	removeReviewer = group user/beast1980
	submit = group user/beast1980
[access "refs/heads/*"]
	read = group user/beast1980
	create = group user/beast1980
	push = +force group user/beast1980
	pushMerge = group user/beast1980
[access "refs/tags/*"]
	push = +force group user/beast1980
	pushTag = group user/beast1980
	pushSignedTag = group user/beast1980
